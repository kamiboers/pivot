<!DOCTYPE html>
<html>
<head>
  <%= render partial: "shared/head" %>
</head>
<body>
  <div class="wrapper">
    <%= render partial: "shared/navbar"%>

    <% flash.each do |type, message| %>
      <%= content_tag :div, message.html_safe, class: type %>
    <% end %>

    <%= yield %>

    <%= render partial: "shared/footer" %>
    <%= render partial: "shared/bscript" %>
  </div>

</body>
</html>
