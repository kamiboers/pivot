<div class="mainbody">
  <div class="container">
    <h1>All Users</h1>
    <div class="col-md-3 user-menu">
      <nav>
        <ul class="nav nav-pills nav-stacked nav-admin">
          <% @active_users.each do |user| %>
          <li role="presentation"><%= link_to user.username, user_path(user.username) %></li>
          <% end %>
        </ul>
      </nav>
    </div>
  </div>
</div>
